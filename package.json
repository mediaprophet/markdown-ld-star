{
  "name": "markdownld",
  "version": "1.4.0",
  "description": "A Node.js library for parsing and generating Markdown-LD* with support for RDF-Star, SPARQL-Star, SHACL, and multiple formats. Includes a playground on GitHub Pages.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["markdown", "rdf", "linked-data", "turtle", "json-ld", "rdf-json", "rdf-star", "sparql-star", "shacl"],
