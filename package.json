{
  "type": "module",
  "name": "markdown-ld-star",
  "version": "1.5.0",
  "description": "A Node.js library for parsing and generating Markdown-LD* with support for RDF-Star, SPARQL-Star, SHACL, and multiple formats. Includes a playground on GitHub Pages.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc && rollup -c && node copy-demos.cjs",
    "test": "jest",
    "test:vitest": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "rdf",
    "linked-data",
    "turtle",
    "json-ld",
    "rdf-json",
    "rdf-star",
    "sparql-star",
    "shacl"
  ],
  "author": "Timothy Holborn",
  "license": "MIT",
  "dependencies": {
    "@comunica/query-sparql": "^4.3.0",
    "@rdfjs/data-model": "^2.1.1",
    "@rdfjs/dataset": "^2.0.2",
    "@rdfjs/parser-n3": "^2.1.0",
    "@rdfjs/serializer-jsonld": "^2.0.1",
    "jsonld": "^8.3.3",
    "n3": "^1.26.0",
    "rdf-ext": "^2.5.2",
    "rdf-parse": "^2.1.0",
    "rdf-serialize": "^4.0.1",
    "rdf-validate-shacl": "^0.6.5",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "shacl-engine": "^1.0.2",
    "streamify-array": "^2.0.0",
    "tslib": "^2.8.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@rdfjs/types": "^2.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/jsonld": "^1.5.15",
    "@types/n3": "^1.26.0",
    "@types/node": "^24.0.14",
    "@types/rdf-ext": "^2.5.2",
    "@types/rdf-js": "^4.0.1",
    "@types/rdfjs__parser-n3": "^2.0.1",
    "@types/rdfjs__serializer-jsonld": "^2.0.1",
    "@types/unist": "^3.0.3",
    "fs-extra": "^11.3.0",
    "jest": "^30.0.4",
    "rollup": "^4.45.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediaprophet/markdown-ld-star.git"
  },
  "bugs": {
    "url": "https://github.com/mediaprophet/markdown-ld-star/issues"
  },
  "homepage": "https://github.com/mediaprophet/markdown-ld-star#readme",
  "overrides": {
    "glob": "^10.4.5"
  }
}
