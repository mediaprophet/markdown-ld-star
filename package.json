{
  "name": "markdown-ld-star",
  "version": "1.5.0",
  "description": "A Node.js library for parsing and generating Markdown-LD* with support for RDF-Star, SPARQL-Star, SHACL, and multiple formats. Includes a playground on GitHub Pages.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["markdown", "rdf", "linked-data", "turtle", "json-ld", "rdf-json", "rdf-star", "sparql-star", "shacl"],
  "author": "Timothy Holborn",
  "license": "MIT",
  "dependencies": {
    "n3": "^1.26.0",
    "@rdfjs/parser-n3": "^2.1.0",
    "@rdfjs/serializer-jsonld": "^2.0.1",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "jsonld": "^8.3.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/rdfjs__parser-n3": "^2.0.0",
    "@types/rdfjs__serializer-jsonld": "^2.0.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "rollup": "^4.45.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/n3": "^1.26.0",
    "@types/jsonld": "^1.5.15",
    "@rdfjs/types": "^2.0.1",
    "@types/unist": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediaprophet/markdown-ld-star.git"
  },
  "bugs": {
    "url": "https://github.com/mediaprophet/markdown-ld-star/issues"
  },
  "homepage": "https://github.com/mediaprophet/markdown-ld-star#readme",
  "overrides": {
    "glob": "^10.4.5"
  }
}